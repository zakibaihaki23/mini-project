AHA;AAIA,KAvDA;AAwDA,IAAA,IAxDA,kBAwDA;AACA,UAAA,KAAA,GAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AAEA,MAAA,KAAA,CACA,IADA,CACA,WADA,EACA;AACA,QAAA,OAAA,EAAA;AACA,UAAA,aAAA,mBAAA,KAAA,CADA;AAEA,0BAAA;AAFA,SADA;AAKA,QAAA,YAAA,EAAA,KAAA,UAAA,CAAA,YALA;AAMA,QAAA,IAAA,EAAA,KAAA,UAAA,CAAA,IANA;AAOA,QAAA,QAAA,EAAA,KAAA,UAAA,CAAA,YAPA;AAQA,QAAA,OAAA,EAAA,KAAA,QAAA,CAAA,OARA;AASA,QAAA,IAAA,EAAA,KAAA,UAAA,CAAA;AATA,OADA,EAaA,IAbA,CAaA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,OAfA,EAgBA,KAhBA,CAgBA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAlBA,EAHA,CAuBA;AACA;AACA;AACA;AACA;;AACA,WAAA,KAAA;AACA;AArFA,GAlFA;AA0KA,EAAA,OA1KA,qBA0KA;AACA,SAAA,UAAA;AACA,SAAA,UAAA,GAFA,CAGA;AACA,GA9KA;AA+KA,EAAA,aA/KA,2BA+KA;AACA,IAAA,aAAA,CAAA,KAAA,QAAA,CAAA;AACA;AAjLA,CAAA","sourcesContent":["<template>\n  <div>\n    <!-- {{ tableData }} -->\n    <v-card>\n      <v-data-table\n        loading=\"isLoading\"\n        loading-text=\"Loading Data, Please Wait......\"\n        :mobile-breakpoint=\"0\"\n        :headers=\"table\"\n        :items=\"tableData\"\n        :items-per-page=\"10\"\n        class=\"elevation-1\"\n        :search=\"search\"\n      >\n        <template v-slot:top>\n          <v-toolbar flat>\n            <v-toolbar-title>\n              <div>\n                Helper List\n              </div>\n            </v-toolbar-title>\n\n            <v-spacer></v-spacer>\n\n            <v-dialog v-model=\"dialog\" max-width=\"500px\">\n              <template v-slot:activator=\"{ on, attrs }\">\n                <v-btn\n                  color=\"primary\"\n                  dark\n                  class=\"mb-2\"\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                >\n                  Registration Helper\n                </v-btn>\n              </template>\n              <v-card>\n                <v-card-title>\n                  <span class=\"headline\">{{ formTitle }}</span>\n                </v-card-title>\n\n                <v-card-text>\n                  <v-container>\n                    <v-row @keyup.enter=\"save\">\n                      <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                        <v-text-field\n                          v-model=\"editedItem.warehouse\"\n                          label=\"Warehouse ID\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                        <v-text-field\n                          v-model=\"editedItem.name\"\n                          label=\"Name\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                        <v-text-field\n                          v-model=\"editedItem.phone_no\"\n                          label=\"Phone No\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                        <v-text-field\n                          v-model=\"editedItem.address\"\n                          label=\"Address\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                        <v-text-field\n                          v-model=\"editedItem.type\"\n                          label=\"Type\"\n                        ></v-text-field>\n                      </v-col>\n                    </v-row>\n                  </v-container>\n                </v-card-text>\n\n                <v-card-actions>\n                  <v-spacer></v-spacer>\n                  <v-btn color=\"blue darken-1\" text @click=\"close\">\n                    Cancel\n                  </v-btn>\n                  <v-btn color=\"blue darken-1\" text @click=\"save\">\n                    Save\n                  </v-btn>\n                </v-card-actions>\n              </v-card>\n            </v-dialog>\n          </v-toolbar>\n          <v-col class=\"d-flex\" cols=\"3\" sm=\"10\">\n            <v-select style=\"margin: 10px\"></v-select>\n\n            <v-select\n              style=\"margin: 10px; border-radius: 50px\"\n              :item=\"tableData\"\n              label=\"Warehouse\"\n            ></v-select>\n\n            <v-text-field\n              style=\"margin: 10px\"\n              v-model=\"search\"\n              append-icon=\"mdi-magnify\"\n              label=\"Search\"\n              single-line\n              hide-details\n            ></v-text-field>\n          </v-col>\n        </template>\n        <template v-slot:item=\"props\">\n          <tr>\n            <td>{{ props.item.code }}</td>\n            <td>{{ props.item.name }}</td>\n            <td>{{ props.item.phone_no }}</td>\n            <td>{{ props.item.address }}</td>\n            <!-- <td>{{ props.item.warehouse.warehouse_name }}</td> -->\n            <td>\n              <div v-if=\"props.item.status == 1\">{{ 'Active' }}</div>\n              <div v-else>{{ 'Inactive' }}</div>\n            </td>\n\n            <td>\n              <v-menu offset-y>\n                <template v-slot:activator=\"{ on: menu }\">\n                  <v-btn small class=\"mr-2\" icon v-on=\"{ ...menu }\"\n                    ><v-icon dark>mdi-dots-vertical</v-icon></v-btn\n                  >\n                </template>\n                <v-list>\n                  <v-list-item>\n                    <v-dialog max-width=\"500px\">\n                      <template\n                        slot:item.actions=\"{ item }\"\n                        v-slot:activator=\"{ on, attrs }\"\n                      >\n                        <v-list-item-title\n                          v-bind=\"attrs\"\n                          v-on=\"on\"\n                          @click=\"editItem(props.item)\"\n                          >Update</v-list-item-title\n                        >\n                      </template>\n                    </v-dialog>\n                  </v-list-item>\n                  <v-list-item>\n                    <v-list-item-title>\n                      Active\n                    </v-list-item-title>\n                  </v-list-item>\n                  <router-view></router-view>\n                </v-list>\n              </v-menu>\n            </td>\n          </tr>\n        </template>\n      </v-data-table>\n    </v-card>\n  </div>\n</template>\n\n<script>\n  import axios from 'axios'\n\n  export default {\n    data() {\n      return {\n        dialog: false,\n        isLoading: true,\n        dialogDelete: false,\n        search: '',\n        edit: '',\n        interval: null,\n        table: [\n          {\n            text: 'Helper ID',\n            value: 'code',\n          },\n          {\n            text: 'Name',\n            value: 'name',\n          },\n          {\n            text: 'Phone No',\n            value: 'phone_no',\n          },\n          {\n            text: 'Address',\n            value: 'address',\n          },\n\n          // {\n          //   text: 'Warehouse',\n          //   value: 'warehouse.warehouse_name',\n          // },\n          {\n            text: 'Status',\n            value: 'status',\n          },\n          {\n            value: 'actions',\n            sortable: false,\n          },\n        ],\n        editData: [\n          {\n            text: 'Edit',\n            value: 'edit',\n          },\n          {\n            text: 'Inactive',\n          },\n        ],\n        tableData: [],\n        editedIndex: -1,\n        editedItem: {\n          warehouse_id: '',\n          name: '',\n          phone_no: '',\n          address: '',\n          type: '',\n        },\n        defaultItem: {\n          warehouse_id: '',\n          name: '',\n          phone_no: '',\n          address: '',\n          type: '',\n        },\n        listPrimitive: null,\n      }\n    },\n\n    computed: {\n      formTitle() {\n        return this.editedIndex === -1 ? 'New Item' : 'Edit Item'\n      },\n    },\n    watch: {\n      dialog(val) {\n        val || this.close()\n      },\n      dialogDelete(val) {\n        val || this.closeDelete()\n      },\n    },\n    methods: {\n      initialize() {\n        this.tableData = [this.tableData]\n      },\n      renderData() {\n        const token = localStorage.getItem('token')\n        axios\n          .get('v1/helper', {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          })\n          .then((response) => {\n            // let that = this;\n\n            this.tableData = response.data.data\n            if (this.tableData === null) {\n              this.tableData = []\n              this.isLoading = false\n            }\n          })\n          .catch((error) => {\n            console.log(error)\n          })\n      },\n      editItem(item) {\n        this.editedIndex = this.tableData.indexOf(item)\n        this.editedItem = Object.assign({}, item)\n        this.dialog = true\n      },\n\n      deleteItem(item) {\n        this.editedIndex = this.tableData.indexOf(item)\n        this.editedItem = Object.assign({}, item)\n        this.dialogDelete = true\n      },\n\n      deleteItemConfirm() {\n        this.tableData.splice(this.editedIndex, 1)\n        this.closeDelete()\n      },\n      close() {\n        this.dialog = false\n        this.$nextTick(() => {\n          this.editedItem = Object.assign({}, this.defaultItem)\n          this.editedIndex = -1\n        })\n      },\n\n      closeDelete() {\n        this.dialogDelete = false\n        this.$nextTick(() => {\n          this.editedItem = Object.assign({}, this.defaultItem)\n          this.editedIndex = -1\n        })\n      },\n      save() {\n        const token = localStorage.getItem('token')\n\n        axios\n          .post('v1/helper', {\n            headers: {\n              Authorization: `Bearer ${token}`,\n              'Content-Type': 'application/json; charset=UTF-8',\n            },\n            warehouse_id: this.editedItem.warehouse_id,\n            name: this.editedItem.name,\n            phone_no: this.editedItem.phone_number,\n            address: this.editItem.address,\n            type: this.editedItem.type,\n          })\n\n          .then(function(response) {\n            console.log(response)\n          })\n          .catch(function(error) {\n            console.log(error)\n          })\n\n        // if (this.editedIndex > -1) {\n        //   Object.assign(this.tableData[this.editedIndex], this.editedItem);\n        // } else {\n        //   this.tableData.push(this.editedItem);\n        // }\n        this.close()\n      },\n    },\n\n    created() {\n      this.renderData()\n      this.initialize()\n      // this.interval = setInterval(this.refreshData, 5000);\n    },\n    beforeDestroy() {\n      clearInterval(this.interval)\n    },\n  }\n</script>\n"],"sourceRoot":"src/views/Helper"}]}