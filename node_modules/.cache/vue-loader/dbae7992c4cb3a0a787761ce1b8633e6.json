moveItem('token')\n        this.$router.push('/helper')\n      },\n    },\n\n    created() {\n      this.getRoutes()\n    },\n    computed: {\n      ...mapGetters({\n        authenticated: 'auth/authenticated',\n        user: 'auth/user',\n      }),\n    },\n  }\n</script>\n\n<style lang=\"scss\">\n  .h-sticky {\n    position: sticky;\n    top: 0;\n    z-index: 2;\n  }\n</style>\n"]}]}